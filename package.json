{
  "name": "husky-pro",
  "version": "1.0.0",
  "description": "",
  "homepage": "https://github.com/dev9-akm/husky-pro#readme",
  "bugs": {
    "url": "https://github.com/dev9-akm/husky-pro/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dev9-akm/husky-pro.git"
  },
  "license": "ISC",
  "author": "",
  "engines": {
    "node": ">=22.13.1"
  },
  "type": "commonjs",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "prepare": "npx husky",
    "lint-staged": "npx lint-staged",
    "typecheck": "npx tsc --noEmit",
    "prettier:format": "npx prettier --write .",
    "prettier:check": "npx prettier --check .",
    "prettier:js": "npx prettier --write '**/*.js'",
    "prettier:ts": "npx prettier --write 'src/**/*.ts'",
    "lint:format": "npx eslint . --fix",
    "lint:check": "npx eslint .",
    "lint:js": "npx eslint '**/*.js' --fix",
    "lint:ts": "npx eslint 'src/**/*.ts' --fix",
    "test": "npx jest --config jest.config.json"
  },
  "dependencies": {
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/express": "^4.17.23",
    "@types/node": "^22.15.31",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^30.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "typescript": "^5.8.3"
  }
}
